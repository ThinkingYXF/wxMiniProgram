<view class="container">
	<!-- <text class="title">10秒游戏</text> -->
	<text class="time">{{s}}.{{s1}}{{s2}}</text>
</view>
<button bind:tap="startGame" size="mini" class="start">开始</button>
<button bind:tap="stopGame" size="mini" class="stop">停止</button>
<button bind:tap="restart" size="mini" class="restart">重新开始</button>
